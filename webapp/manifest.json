{
  "_version": "1.49.0",
  "sap.app": {
    "id": "commandearticle",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:listdetail",
      "version": "1.8.6",
      "toolsId": "43d276f2-bb10-46b9-9021-887885fe27f0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZGW_MBE_MMF_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.113.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "commandearticle.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
          {
            "uri": "css/commande_ael_article.css"
          },
          {
            "uri": "css/demande_ame_project.css"
          },
          {
            "uri": "css/vendors.css"
          }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "viewPath": "commandearticle.view",
        "controlId": "layout",
        "controlAggregation": "beginColumnPages",
        "bypassed": {
          "target": [
            "notFound"
          ]
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "home",
          "target" : "home"
        },
        {
          "pattern": "ArticleeSet",
          "name": "list",
          "target": "list"
        },
        {
          "pattern": "ArticleeSet/{objectId}",
          "name": "object",
          "target": [
            "list",
            "object"
          ]
        },
        {
          "name": "RouteDA",
          "pattern": "DA",
          "target": [
            "TargetDA"
          ]
        },
        {
          "name": "RouteDASingle",
          "pattern": "DASingle/{object}",
          "target": [
            "TargetDASingle"
          ]
        },
        {
          "name": "RouteCA",
          "pattern": "CA",
          "target": [
            "TargetCA"
          ]
        },
        {
          "name": "RouteCASingle",
          "pattern": "CASingle/{object}",
          "target": [
            "TargetCASingle"
          ]
        },
        {
          "name": "RouteVendorsMain",
          "pattern": "vendors",
          "target": [
            "TargetVendorsMain"
          ]
        },
        {
          "name": "RouteVendorSingleObjectPage",
          "pattern": "vendors/{id}",
          "target" : [
              "TargetVendorSingleObjectPage"
          ]
        },
        {
          "name": "Fournisseurs",
          "pattern": "Fournisseurs",
          "target": [
            "TargetFournisseurs"
          ]
        }
      ],
      "targets": {
        "home" : {
          "viewName": "Home",
          "viewLevel": 1,
          "viewId": "home"
        },
        "list": {
          "viewName": "List",
          "viewLevel": 1,
          "viewId": "list"
        },
        "object": {
          "viewName": "Detail",
          "viewId": "detail",
          "viewLevel": 1,
          "controlAggregation": "midColumnPages"
        },
        "detailObjectNotFound": {
          "viewName": "DetailObjectNotFound",
          "viewId": "detailObjectNotFound",
          "controlAggregation": "midColumnPages"
        },
        "notFound": {
          "viewName": "NotFound",
          "viewId": "notFound"
        },
        "TargetDA": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "DA",
          "viewName": "DA"
        },
        "TargetDASingle": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "DASingle",
          "viewName": "DASingle"
        },
        "TargetCA": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "CA",
          "viewName": "CA"
        },
        "TargetCASingle": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "CASingle",
          "viewName": "CASingle"
        }, 
        "TargetVendorsMain": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "VendorsMain",
          "viewName": "VendorsMain"
        },
        "TargetVendorSingleObjectPage": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Vendor",
          "viewName": "Vendor"
        },
        "TargetFournisseurs" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Fournisseurs",
          "viewName": "Fournisseurs"
        }
      }
    },
    "rootView": {
      "viewName": "commandearticle.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    }
  }
}
